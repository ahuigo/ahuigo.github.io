{
  "title": "redline shortcuts for mac osx...",
  "rules": [
    {
      "description": "redline shortcuts for mac osx...",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": {
              "key_code": "up_arrow"
         }
        },{
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": {
              "key_code": "down_arrow"
         }
        },{
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": {
              "key_code": "delete_or_backspace",
              "modifiers": [
                "option"
              ]
            }
        },{
          "type": "basic",
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": { "mandatory": [ "option" ] }
          },
          "to": {
              "key_code": "w",
              "modifiers": [ "control" ]
          }
        },{
          "type": "basic",
          "from": {
            "key_code": "u",
            "modifiers": { "mandatory": [ "control" ] }
          },
          "to": {
              "key_code": "delete_or_backspace",
              "modifiers": [
                "command"
              ]
            }
        },{
          "type": "basic",
          "from": { "key_code": "f10", "modifiers": { "mandatory": [ "left_control" ] }
          },
          "to": {
              "key_code": "volume_decrement"
           }
        }
      ]
    }
  ]
}
